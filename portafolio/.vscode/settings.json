{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "tsconfig.json": "tsconfig.*.json, env.d.ts",
    "vite.config.*": "jsconfig*, vitest.config.*, cypress.config.*, playwright.config.*",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .oxlint*, oxlint*, .prettier*, prettier*, .editorconfig"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5f1c67",
    "activityBar.background": "#5f1c67",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#7d7422",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#5f1c67",
    "statusBar.background": "#3a113f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#5f1c67",
    "statusBarItem.remoteBackground": "#3a113f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#3a113f",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#3a113f99",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
